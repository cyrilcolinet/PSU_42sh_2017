[001]
NAME="Nothing"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo ''
[001-END]

[002]
NAME="Exec two command"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo 'ls /etc'
  echo 'cat /etc/resolv.conf'
[002-END]

[003]
NAME="Testing with /usr/bin PATH"
SETUP="export TERM=xterm ; PATH='/usr/bin'"
CLEAN=""
TESTS=
  echo 'pwd'
[003-END]

[004]
NAME="Testing with /bin PATH"
SETUP="export TERM=xterm ; PATH='/bin'"
CLEAN=""
TESTS=
  echo 'pwd'
[004-END]

[005]
NAME="run commands"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo 'cat /etc/resolv.conf'
  sleep 0.5
  echo 'ls -l /etc'
  sleep 0.5
  echo 'pwd'
[005-END]

[006]
NAME="setenv and unsetenv"
SETUP="/usr/bin/env > .lstenv ; while read varenv ; do unset $varenv ; done < <(/bin/cat .lstenv | /usr/bin/cut -f1 -d=) ; export PATH=/bin:/usr/bin ; export _=ls ; export LS_COLORS=RID ; export MYBG=4 ; export LVL=4 ; export NOTE=2"
CLEAN="while read varenv ; do export "$varenv" 2>/dev/null ; done  < <(/bin/cat .lstenv) ; /bin/rm -f .lstenv"
TESTS=
  echo 'unsetenv HOSTTYPE'
  echo 'unsetenv VENDOR'
  echo 'unsetenv OSTYPE'
  echo 'unsetenv MACHTYPE'
  echo 'unsetenv SHLVL'
  echo 'unsetenv PWD'
  echo 'unsetenv LOGNAME'
  echo 'unsetenv USER'
  echo 'unsetenv GROUP'
  echo 'unsetenv HOST'
  echo 'unsetenv REMOTEHOST'
  echo 'unsetenv _'
  echo 'unsetenv LS_COLORS'
  echo 'unsetenv NOTE'
  echo 'setenv titi'
  echo 'setenv tata coucou'
  echo 'env'
  echo '/usr/bin/env'
  echo 'setenv var1 val1'
  echo 'setenv var2 val2'
[006-END]

[007]
NAME="builtin cd"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo 'cd ..'
  echo 'ls -l'
  echo 'cd /usr'
  echo 'ls -l'
[007-END]

[008]
NAME="many tab and space"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo -e "\t\t    ls\t     \t-l    \t\t  \t -a /usr   \t\t"
[008-END]

[009]
NAME="Launch DivZero program with core dump"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo "./my_divzero"
[009-END]

[010]
NAME="Launch SegFault program without core dump"
SETUP="ulimit -c 0 ; export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo "./my_segfault"
[010-END]

[011]
NAME="Testing simple pipe"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo "ls -l /etc | grep sh"
[011-END]

[012]
NAME="A pipe with a builtin ?"
SETUP="export VAR1=VALEUR1 ; OLDPATH=$PATH ; PATH='/random'"
CLEAN="PATH=$OLDPATH"
TESTS=
  echo 'setenv | /bin/grep VAR1'
  echo '/bin/ls | cd /usr'
  echo 'ls -l'
  echo 'exit 45|cat /etc/resolv.conf'
[012-END]

[013]
NAME="Launch Floating Exception program without core dump"
SETUP="ulimit -c 0 ; export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo "./my_floating_exp"
[013-END]

[014]
NAME="Launch incompatible binary"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo "./incompatible"
[014-END]

[015]
NAME="Execute a directory (with exec perms)"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin' ; mkdir dir ; chmod +x dir"
CLEAN="rm -rf dir"
TESTS=
  echo "./dir"
[015-END]

[016]
NAME="Execute a directory (without exec perms)"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin' ; mkdir dir"
CLEAN="rm -rf dir"
TESTS=
  echo "./dir"
[016-END]

[017]
NAME="Local variables not in env"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo "set OUI=non"
  echo "echo \$OUI"
  echo "unset OUI"
  echo "echo \$OUI"
  echo "set BLABLABLA=peut-etre"
  echo "echo \$BLABLABLA"
  echo "set BLABLABLA=test"
  echo "printenv OUI"
  echo "unset OUI"
[017-END]

[018]
NAME="Empty environment"
SETUP="/usr/bin/env > .lstenv ; while read varenv ; do unset $varenv ; done < <(/bin/cat .lstenv | /usr/bin/cut -f1 -d=) ; export PATH=/bin:/usr/bin ; export _=ls ; export LS_COLORS=RID ; export MYBG=4 ; export LVL=4 ; export NOTE=2"
CLEAN="while read varenv ; do export "$varenv" 2>/dev/null ; done  < <(/bin/cat .lstenv) ; /bin/rm -f .lstenv"
TESTS=
  echo 'unsetenv HOSTTYPE'
  echo 'unsetenv VENDOR'
  echo 'unsetenv OSTYPE'
  echo 'unsetenv MACHTYPE'
  echo 'unsetenv SHLVL'
  echo 'unsetenv PWD'
  echo 'unsetenv LOGNAME'
  echo 'unsetenv USER'
  echo 'unsetenv GROUP'
  echo 'unsetenv HOST'
  echo 'unsetenv REMOTEHOST'
  echo 'unsetenv _'
  echo 'unsetenv LS_COLORS'
  echo 'unsetenv NOTE'
  echo 'env'
  echo 'ls -l'
  echo 'cd'
  echo 'cd -'
[018-END]

[019]
NAME="run commands with ; separator"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo 'ls -l ; ; ; ; ;; ; ; ; ; ;;;;; echo OUI'
[019-END]

[020]
NAME="simple aliases"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo 'alias ll ls -l'
  echo 'll'
  echo 'll'
  echo 'll'
[020-END]

[021]
NAME="alias in alias"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo 'alias ll ls -l'
  echo 'll'
  echo 'll'
  echo 'll'
  echo 'alias ll ll'
  echo 'alias crash ll ; ls -l'
[021-END]

[022]
NAME="Infinite loop local variable"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo "set OUI=non"
  echo "set \$OUI=$OUI"
  echo "unset OUI"
  echo "set OUI=\$OUI"
[022-END]

[023]
NAME="Only space"
SETUP=""
CLEAN=""
TESTS=
  echo "                                                    "
[023-END]

[024]
NAME="Only tabulation"
SETUP=""
CLEAN=""
TESTS=
  echo "\\t\\t\\t\\t"
[024-END]

[025]
NAME="Only tabulation and spaces"
SETUP=""
CLEAN=""
TESTS=
  echo "   \\t\\t            \\t\\t"
[025-END]

[026]
NAME="Tabulation and spaces and command"
SETUP=""
CLEAN=""
TESTS=
  echo "   \\t\\t       ls -l     \\t\\t"
[026-END]